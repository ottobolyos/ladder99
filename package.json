{
  "name": "ladder99",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "nx run-many --all --target=build",
    "build:start": "pnpm build && npm start",
    "build:start:clean": "pnpm clean && pnpm build && pnpm start",
    "build:start:clean:debug": "pnpm clean && pnpm build && pnpm start:debug",
    "build:start:debug": "pnpm build && pnpm start:debug",
    "clean": "rimraf dist tmp",
    "commit:check": "commitlint",
    "deps:update": "bash scripts/update_dependencies.sh",
    "docs:api": "pnpm serve & curl --retry 10 --retry-all-errors --retry-delay 1 -sLo docs/api/api.json \"http://localhost:${L99_GW_PORT:-3333}/${L99_GW_API_PREFIX:-api}/${L99_GW_SWAGGER_PATH:-swagger}-json\"; curl -X PUT \"http://localhost:${L99_GW_PORT:-3333}/${L99_GW_API_PREFIX:-api}/app/exit\"",
    "lint:check": "nx run-many --all --target=lint",
    "lint:check:errors": "nx run-many --all --target=lint --quiet",
    "lint:fix": "nx run-many --all --target=lint --fix",
    "postbuild": "bash scripts/update_package_version.sh",
    "serve": "nx run-many --all --target=serve --inspect false --watch false",
    "serve:debug": "nx run gateway:serve --inspect true --host 0.0.0.0 --port 9229 --watch false'",
    "serve:debug:watch": "nx run gateway:serve --inspect true --host 0.0.0.0 --port 9229 --watch true",
    "serve:watch": "nx run gateway:serve --inspect false --watch true",
    "start": "node dist/apps/gateway/main",
    "start:debug": "node --inspect-brk=0.0.0.0:9229 dist/apps/gateway/main",
    "style:check": "nx format:check",
    "style:fix": "nx format:write",
    "test": "nx run-many --all --target=test",
    "test:cov": "nx run-many --all --target=test --coverage",
    "test:cov:verb": "nx run-many --all --target=test --coverage --verbose",
    "test:debug": "nx run-many --all --target=test",
    "test:debug:verb": "nx run-many --all --target=test --verbose",
    "test:e2e": "nx run-many --all --target=e2e",
    "test:e2e:verb": "nx run-many --all --target=e2e --verbose",
    "test:verb": "nx run-many --all --target=test --verbose",
    "test:watch": "nx run-many --all --target=test --watch",
    "test:watch:verb": "nx run-many --all --target=test --watch --verbose"
  },
  "private": true,
  "dependencies": {
    "@fastify/static": "6.9.0",
    "@nestjs/common": "9.3.8",
    "@nestjs/config": "2.3.1",
    "@nestjs/core": "9.3.8",
    "@nestjs/mapped-types": "1.2.2",
    "@nestjs/platform-fastify": "9.3.8",
    "@nestjs/swagger": "6.2.1",
    "@ogma/logger": "2.5.0",
    "@ogma/nestjs-module": "4.2.2",
    "@ogma/platform-fastify": "4.0.1",
    "@ogma/platform-tcp": "4.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.0",
    "swagger-themes": "1.2.26",
    "tslib": "2.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.3",
    "@commitlint/config-conventional": "17.4.3",
    "@commitlint/prompt-cli": "17.4.3",
    "@nestjs/schematics": "9.0.4",
    "@nestjs/testing": "9.3.8",
    "@nrwl/eslint-plugin-nx": "15.7.0",
    "@nrwl/jest": "15.7.0",
    "@nrwl/linter": "15.7.0",
    "@nrwl/nest": "15.7.0",
    "@nrwl/node": "15.7.0",
    "@nrwl/webpack": "15.7.0",
    "@nrwl/workspace": "15.7.0",
    "@types/jest": "29.4.0",
    "@types/node": "18.13.0",
    "@types/validator": "13.7.12",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "axios": "1.3.3",
    "commitlint-plugin-selective-scope": "1.0.1",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-sort-keys-custom-order": "1.0.5",
    "eslint-plugin-tsdoc": "0.2.17",
    "eslint-plugin-unicorn": "45.0.2",
    "jest": "29.4.2",
    "jest-environment-jsdom": "29.4.2",
    "nx": "15.7.0",
    "prettier": "2.8.4",
    "rimraf": "4.1.2",
    "source-map-support": "0.5.21",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  }
}
